// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;

int main() {
   int day=7;
   int arr[day];
   int total = 0;
   double average;
   for(int i=1; i<day; i++){
       cout<<"Day"<<" "<<i<<"duration of exercise";
       cin>>arr[i];
       total+=arr[i];
   }
   average = (double)total/day;
   cout<<average;
    return 0;
}
