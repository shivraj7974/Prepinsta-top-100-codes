#include<iostream>
using namespace std;

void findmax(int arr[], int n)
{
    int max=-1, maxind=-1;
    for(int i=0; i<n ; i++){
        if(arr[i]>max)
        {
            max=arr[i];
            maxind=i;
        }
    }
    cout<<max<<endl<<maxind<<endl;
}

int main(){
    int n;
    cin>>n;
    int arr[n];
    for(int i=0; i<n; i++){
        cin>>arr[i];
    }
  findmax(arr,n);
}
