#include<bits/stdc++.h>
using namespace std;
void asdsfun(int arr[], int n)
{
    sort(arr, arr+ n);
    for(int i=0; i<n/2; i++)
    cout<<arr[i]<<endl;
    for(int j=n-1; j>n/2; j--)
    cout<<arr[j]<<endl;
    
}
int main(){
    int arr[]={122,44,56,67,88,589,99};
    int n=sizeof(arr)/sizeof(arr[0]);
    asdsfun(arr,n);
}
